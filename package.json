{
  "name": "snapsheet-string-utils",
  "version": "1.0.0",
  "main": "lib/index",
  "jsnext:main": "es6/index",
  "engines": {
    "node": "^6.1.0",
    "yarn": ">=0.18.0"
  },
  "scripts": {
    "build": "npm run build-cjs && npm run build-es6 && rimraf umd && npm run build-umd && npm run build-min",
    "build-cjs": "rimraf lib && babel ./src -d lib",
    "build-es6": "rimraf es6 && babel ./src -d es6 --blacklist=es6.modules",
    "build-umd": "webpack --env=development src/index.js umd/snapsheet-string-utils.js",
    "build-min": "webpack --env=production src/index.js umd/snapsheet-string-utils.min.js",
    "lint": "eslint src",
    "test": "mocha --compilers js:babel-core/register \"./tests/**/*.js\" --recursive",
    "demo": "webpack-dev-server --progress --port 3000 --content-base demo --inline --config demo/webpack.config.js"
  },
  "files": [
    "*.md",
    "es6/*",
    "es6/*/**",
    "lib/*",
    "lib/*/**",
    "umd/*",
    "umd/*/**",
    "src/*",
    "src/*/**"
  ],
  "repository": "git@github.com:bodyshopbidsdotcom/snapsheet-string-utils.git",
  "author": "Snapsheet Inc.",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
    "eslint-config-snapsheet": "^1.0.0",
    "eslint-plugin-react": "^6.9.0",
    "mocha": "^3.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.5.4",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
